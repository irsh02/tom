<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="testdata">

	<test name="BothCorrect">
	<parameter name="Username" value="Admin"></parameter>
		<parameter name="Password" value="admin123"></parameter>
		<classes>
			<class name="datadrivenfw.BothCorrect" />
		</classes>
	</test>

	<test name="BothIncorrect">
	<parameter name="Username" value="debrs"></parameter>
		<parameter name="Password" value="sana"></parameter>
		<classes>
			<class name="datadrivenfw.BothIncorrect" />
		</classes>
	</test>

	<test name="CorrectPasword">
	<parameter name="Username" value="Admin12"></parameter>
		<parameter name="Password" value="admin123"></parameter>
		<classes>
			<class name="datadrivenfw.CorrectPasword" />
		</classes>
	</test>

	<test name="correctusername">
	<parameter name="Username" value="Admin"></parameter>
		<parameter name="Password" value="admit"></parameter>
		<classes>
			<class name="datadrivenfw.correctusername" />
		</classes>
	</test>

</suite>